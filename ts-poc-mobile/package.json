        "@nivinjoseph/n-ject": "^1.0.21",
        "@nivinjoseph/n-log": "^1.0.3",
        "@nivinjoseph/n-util": "^1.0.18",
        "@nivinjoseph/n-validate": "^1.0.12",
        "@types/jquery": "^3.3.4",
        "@types/mocha": "^2.2.48",
